using JUPITERPA_BALANCE_TRANSACTION as JUPITERPA_BALANCE_TRANSACTION;
using BALANCESERVICE_ACCOUNT as BALANCESERVICE_ACCOUNT;
view BALANCESERVICE_TRANSACTION as select from JUPITERPA_BALANCE_TRANSACTION as TRANSACTION mixin {
  __ACCOUNT : association to BALANCESERVICE_ACCOUNT on __ACCOUNT.ID = $projection.ACCOUNT_ID;
} into {
  TRANSACTION.ID,
  __ACCOUNT as ACCOUNT,
  TRANSACTION.AMOUNT,
  TRANSACTION.ACCOUNT_ID
};
