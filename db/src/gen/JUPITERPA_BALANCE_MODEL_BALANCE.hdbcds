using JUPITERPA_BALANCE_MODEL_TRANSACTION as JUPITERPA_BALANCE_MODEL_TRANSACTION;
view JUPITERPA_BALANCE_MODEL_BALANCE as select from JUPITERPA_BALANCE_MODEL_TRANSACTION as TRANSACTION {
  key ACCOUNT.ID as ID,
  ACCOUNT.NAME,
  sum(AMOUNT_VALUE) as AMOUNT : Integer,
  AMOUNT_UNIT as UNIT : Integer
} group by ACCOUNT.ID, ACCOUNT.NAME, AMOUNT_UNIT;
