using JUPITERPA_BALANCE_MODEL_ACCOUNT as JUPITERPA_BALANCE_MODEL_ACCOUNT;
entity JUPITERPA_BALANCE_MODEL_TRANSACTION {
  key ID : String(36);
  ACCOUNT : association to JUPITERPA_BALANCE_MODEL_ACCOUNT on ACCOUNT.ID = ACCOUNT_ID;
  AMOUNT_VALUE : Integer;
  AMOUNT_UNIT : String(10);
  ADMIN_CREATEDAT : UTCTimestamp;
  ADMIN_CREATEDBY : String(128);
  ADMIN_CHANGEDAT : UTCTimestamp;
  ADMIN_CHANGEDBY : String(128);
  ACCOUNT_ID : String(36);
};
